<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.flora.model.FlowerAd" table="flower_ad" lazy="false">
		<id name="id" type="integer" column="recordid">
			<generator class="native" />
		</id>
		<property name="imgName" type="string" column="img_name"/>
		<property name="tag" type="string" column="tag"/>
		<property name="link" type="string" column="link" />
		<property name="sortNum" type="integer" column="sort_num" />
		<property name="status" type="string" column="status"/>
		<property name="createTime" type="date" column="create_time" not-null="true"/>
	</class>
</hibernate-mapping>
